{
  "build": {
    "arduino": {
      "ldscript": "eagle.flash.1m128.ld"
    },
    "core": "esp8266",
    "extra_flags": "-DESP8266 -DARDUINO_ARCH_ESP8266 -DARDUINO_ESP8266_SONOFF_BASIC -DESP8285 -DAFE_ESP_FLASH_1MB",
    "f_cpu": "80000000L",
    "f_flash": "40000000L",
    "flash_mode": "dout",
    "mcu": "esp8266",
    "variant": "itead"
  },
  "connectivity": [
    "wifi"
  ],
  "frameworks": [
    "arduino",
    "esp8266-rtos-sdk",
    "esp8266-nonos-sdk"
  ],
  "name": "Sonoff Basic ESP82xx 1MB customized for AFE Firmware",
  "upload": {
    "maximum_ram_size": 81920,
    "maximum_size": 1048576,
    "require_upload_port": true,
    "resetmethod": "ck",
    "speed": 115200
  },
  "url": "https://www.itead.cc/sonoff-wifi-wireless-switch.html",
  "vendor": "ITEAD"
}